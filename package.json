    "webpack": "webpack"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-transform-async-to-generator": "^7.4.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/runtime": "^7.4.2",
    "airdcpp-extension": "^1.1.0-beta",
    "airdcpp-extension-settings": "^1.1.0-beta",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "nodemon": "^1.18.10",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.11",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "main": "dist/main.js",
  "airdcpp": {
    "apiVersion": 1,
    "minApiFeatureLevel": 0
  }
